<h1>Editing university</h1>

<% form_for(@university) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :university_name %><br />
    <%= f.text_field :university_name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @university %> |
<%= link_to 'Back', universities_path %>